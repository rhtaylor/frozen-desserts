{
  "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/<APP_NAME>_ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "<APP_NAME>",
      "image": "<IMAGE_NAME>",
      "essential": true,
      "portMappings": [
        {  
          "protocol": "tcp",
          "hostPort": <SERVICE_PORT> ,
          "containerPort": <SERVICE_PORT>
        }
      ]
    }
  ],
  "requiresCompatibilities": [
      "FARGATE" 
  ],
  "cpu": "<CPU>",
  "memory": "<MEMORY>",
  "networkMode": "awsvpc",
  "family": "<APP_NAME>"
}